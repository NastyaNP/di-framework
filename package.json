{
  "name": "ts-di-framework",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "ts-patch install",
    "log": "tspc --all",
    "ts-compile": "tspc --build --verbose",
    "start-server": "node src/server.js",
    "preview": "vite preview",
    "dev": "vite",
    "debug": "vite --debug true --clearScreen false",
    "watch": "npm run ts-compile && vite build --watch",
    "build": "npm run ts-compile && vite build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "ts-patch": "^3.1.2",
    "tst-reflect-transformer": "^1.0.0-alpha.11",
    "typescript": "^5.1.3",
    "vite": "^5.2.0",
    "vite-plugin-typescript-transform": "^1.3.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "tst-reflect": "^0.12.0",
    "ttypescript": "^1.5.15",
    "typescript-rtti": "^0.9.6"
  }
}
